{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ],
  "redirects": [
    {
      "source": "/:path*",
      "destination": "https://zhengyutouzi.com/:path*",
      "permanent": false,
      "has": [
        {
          "type": "host",
          "value": "www.zhengyutouzi.com"
        }
      ]
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        }
      ]
    }
  ]
}